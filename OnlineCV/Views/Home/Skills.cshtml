@{
    ViewBag.Title = "Home Page";
    Layout = null;
    
}
<div style="margin-top:60px;">
    <div>
        <div class="row">
            <div class="col-md-3 col-xs-1 cvTitle">
               TECHNICAL SKILL SET
                <hr style="height:4px;width:50%;background-color:black;margin-left:0px;">
            </div>
        </div>
        <div class="row">
            <div class="col-md-8 col-sm-12">
                <ul class="cvKeySkills">
                    <li class="cvKeySkills" ng-repeat="skill in mycv.skills">
                        <div class="cvSubTitle">
                            {{skill.heading}}
                        </div>
                        <div class="skillDesc">
                            {{skill.description}}
                        </div>

                    </li>
                </ul>
            </div>
            <div class="col-md-4 hidden-xs">
            </div>           
        </div>


    </div>


</div>